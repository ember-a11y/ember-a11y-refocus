{
  "name": "workspace-root",
  "version": "4.1.4",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/ember-a11y/ember-a11y-refocus.git"
  },
  "license": "MIT",
  "author": "",
  "scripts": {
    "build": "pnpm --filter ember-a11y-refocus build",
    "lint": "pnpm --filter '*' lint",
    "lint:fix": "pnpm --filter '*' lint:fix",
    "prepare": "pnpm build",
    "start": "concurrently 'pnpm:start:*' --restart-after 5000 --prefix-colors cyan,white,yellow",
    "start:addon": "pnpm --filter ember-a11y-refocus start",
    "start:test-app": "pnpm --filter test-app start",
    "test": "pnpm --filter '*' test"
  },
  "devDependencies": {
    "@glint/core": "^1.5.1",
    "concurrently": "^9.1.2",
    "prettier": "^3.4.2",
    "prettier-plugin-ember-template-tag": "^2.0.4",
    "release-plan": "^0.16.0"
  },
  "packageManager": "pnpm@10.1.0+sha512.c89847b0667ddab50396bbbd008a2a43cf3b581efd59cf5d9aa8923ea1fb4b8106c041d540d08acb095037594d73ebc51e1ec89ee40c88b30b8a66c0fae0ac1b",
  "engines": {
    "pnpm": ">= 10.0.0"
  }
}
